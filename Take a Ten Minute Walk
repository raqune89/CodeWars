def is_valid_walk(walk):
    north_south, east_west = 0, 0
    if len(walk) == 10:
        for i in walk:
            if i == 'n': north_south += 1
            if i == 's': north_south -= 1
            if i == 'w': east_west += 1
            if i == 'e': east_west -= 1
    else:
        return False
    return north_south == 0 and east_west == 0
